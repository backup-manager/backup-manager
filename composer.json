{
    "name": "heybigname/backup-manager",
    "version": "0.3.7",
    "description": "Database backup manager for dumping to and restoring databases from S3, Dropbox, FTP, and SFTP.",
    "authors": [
        {
            "name": "Shawn McCool",
            "email": "shawn@heybigname.com",
            "homepage": "http://heybigname.com/"
        },
        {
            "name": "Mitchell van Wijngaarden",
            "email": "mitchell@kooding.nl",
            "homepage": "http://heybigname.com/"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/process": "2.*",
        "aws/aws-sdk-php": "~2.4",
        "league/flysystem": ">=0.3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "dev-master",
        "satooshi/php-coveralls": "~0.6",
        "illuminate/support": "4.x",
        "illuminate/container": "4.x",
        "illuminate/console": "4.x",
        "rackspace/php-opencloud": "1.9.*",
        "dropbox/dropbox-sdk": "1.1.*",
        "phpspec/phpspec": "~2.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "suggest": {
        "aws/aws-sdk-php": "Enables AWS S3 storage",
        "dropbox/dropbox-sdk": "Enables Dropbox storage",
        "phpseclib/phpseclib": "Enables SFTP storage",
        "sabre/dav": "Enables WebDav storage",
        "rackspace/php-opencloud": "Enables Rackspace Cloud storage"
    },
    "autoload": {
        "psr-4": {
            "BigName\\BackupManager\\": "src/"
        }
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.3-dev"
        }
    }
}
