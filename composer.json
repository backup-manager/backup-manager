{
  "name": "backup-manager/backup-manager",
  "description": "A framework agnostic database backup manager with user-definable procedures and support for S3, Dropbox, FTP, SFTP, and more with drivers for popular frameworks.",
  "authors": [
    {
      "name": "Shawn McCool",
      "email": "shawn@heybigname.com",
      "homepage": "http://heybigname.com/"
    },
    {
      "name": "Mitchell van Wijngaarden",
      "email": "mitchell@kooding.nl",
      "homepage": "http://heybigname.com/"
    },
    {
      "name": "St√©phane Demonchaux",
      "email": "demonchaux.stephane@gmail.com",
      "homepage": "https://github.com/fezfez"
    }
  ],
  "require": {
    "php":  "~8.0.0 || ~8.1.0",
    "symfony/process": "^4.4 || ^5.0 || ^6.0"
  },
  "require-dev": {
    "doctrine/coding-standard": "^9.0",
    "league/flysystem": "^1.0.64",
    "phpunit/phpunit": "^9.5",
    "vimeo/psalm": "^4.23"
  },
  "suggest": {
    "league/flysystem": "league flysystem adapter"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "autoload": {
    "psr-4": {
      "Fezfez\\BackupManager\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fezfez\\BackupManager\\Tests\\": "spec/"
    }
  },
  "license": "MIT"
}
