{
  "name": "district09/backup-manager",
  "description": "A framework agnostic database backup manager with user-definable procedures and support for S3, Dropbox, FTP, SFTP, and more with drivers for popular frameworks.",
  "license": "MIT",
  "authors": [
    {
      "name": "Shawn McCool",
      "email": "shawn@heybigname.com",
      "homepage": "http://heybigname.com/"
    },
    {
      "name": "Mitchell van Wijngaarden",
      "email": "mitchell@kooding.nl",
      "homepage": "http://heybigname.com/"
    },
    {
      "name": "Jelle Sebreghts",
      "email": "Jelle.Sebreghts@district09.gent",
      "homepage": "https://district09.gent/"
    }
  ],
  "require": {
    "php": ">=8.0",
    "league/flysystem": "^2.1.1 || ^3.0",
    "symfony/process": "^5.4 || ^6.0"
  },
  "require-dev": {
    "league/flysystem-async-aws-s3": "^2.0.6 || ^3.0",
    "league/flysystem-aws-s3-v3": "^2.0 || ^3.0",
    "league/flysystem-ftp": "^2.4 || ^3.0",
    "league/flysystem-google-cloud-storage": "^2.0 || ^3.0",
    "league/flysystem-sftp-v3": "^2.0 || ^3.0",
    "league/flysystem-webdav": "^3.0",
    "marcandreappel/flysystem-backblaze": "^2.1",
    "phpspec/phpspec": "^7.2",
    "spatie/flysystem-dropbox": "^2.0 || ^3.0"
  },
  "suggest": {
    "league/flysystem-aws-s3-v3": "AWS S3 adapter support.",
    "league/flysystem-google-cloud-storage": "Google Cloud Storage adapter support.",
    "league/flysystem-rackspace": "Rackspace adapter support.",
    "league/flysystem-sftp": "SFTP adapter support.",
    "league/flysystem-webdav": "Webdav adapter support.",
    "marcandreappel/flysystem-backblaze": "B2 adapter support.",
    "spatie/flysystem-dropbox": "Dropbox API v2 adapter support."
  },
  "autoload": {
    "psr-4": {
      "District09\\BackupManager\\": "src/"
    }
  }
}
